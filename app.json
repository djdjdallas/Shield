{
  "expo": {
    "name": "Defendr",
    "slug": "defendr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E0E7FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.defendr.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSClipboardUsageDescription": "This app needs clipboard access to analyze suspicious messages you've copied.",
        "NSUserTrackingUsageDescription": "This app does not track you. All analysis is done anonymously.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ],
        "NSPrivacyCollectedDataTypes": [],
        "NSPrivacyTracking": false,
        "NSPrivacyTrackingDomains": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "package": "com.defendr.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET"
      ],
      "edgeToEdgeEnabled": true,
      "blockedPermissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "defendr",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "126fc08e-7ac4-4f23-9dee-9c15b8e2b197"
      },
      "SUPABASE_URL": "",
      "SUPABASE_ANON_KEY": "",
      "router": {}
    },
    "hooks": {
      "postPublish": []
    },
    "privacy": "unlisted",
    "primaryColor": "#8B5CF6",
    "description": "AI-powered text scam detection app. Protect yourself from phishing, fraud, and smishing with instant message analysis.",
    "githubUrl": "https://github.com/djdallas/defendr",
    "owner": "djdallas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/126fc08e-7ac4-4f23-9dee-9c15b8e2b197"
    }
  }
}
